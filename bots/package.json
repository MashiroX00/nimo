{
  "name": "docker-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot for controlling Docker containers via Docker Management API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node --esm src/index.ts",
    "build": "tsc -p tsconfig.json",
